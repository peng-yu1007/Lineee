@model Lineee.Models.ExaOrders

@{
    ViewBag.Title = "Choose";
}

<h2>Choose</h2>
<div>
    <h4>病歷索引檔</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            <p>病歷號</p>
        </dt>

        <dd>
            @ViewBag.patient_id
        </dd>

        <dt>
            <p>身分證</p>
        </dt>

        <dd>
            @ViewBag.id_card
        </dd>

        <dt>
            <p>病人姓名</p>
        </dt>

        <dd>
            @ViewBag.patient_name
        </dd>

        <dt>
            <p>病人生日</p>
        </dt>

        <dd>
            @ViewBag.birth_date
        </dd>

        <dt>
            <p>病人血型</p>
        </dt>

        <dd>
            @ViewBag.blood
        </dd>

        <dt>
            <p>病人性別</p>
        </dt>

        <dd>
            @ViewBag.gender
        </dd>

    </dl>
</div>



<div>
    @Html.ActionLink("Back to List", "Index")
</div>
